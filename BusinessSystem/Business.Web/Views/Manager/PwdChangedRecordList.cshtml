@{
    ViewBag.Title = "<li><i class=\"\"></i><a href=\"/Manager/ManagerList\">服务中心</a></li><li class=\"active\">用户管理</li>";
}
@using Business.Core
@model IList<PwdChangeRecord>
 
<div class="row-fluid">
    <div class="span12">
        <div class="widget-box">
            <div class="widget-header widget-header-blue widget-header-flat">
                <h4 class="lighter">密码修改记录</h4>
                <div class="widget-toolbar">
                     
                </div>
            </div>
            <div class="widget-body">
                <div class="table-responsive">
                    <table id="sample-table-1" class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr> 	 	 	
                                <th class="center">
                                    <label>
                                        <span class="lbl">序号</span>
                                    </label>
                                </th>
                                <th>登陆用户名</th>
                                <th>被修改用户名</th>
                                <th class="hidden-480">IP地址</th>
                                <th>
                                    修改时间
                                </th>
                                 
                            </tr>
                        </thead>

                        <tbody>
                            @if (Model != null &&Model.Count!= 0)
                            {
                                int index = 0;
                                foreach (var item in Model)
                                {
                                    index = index + 1;
                                    <tr>
                                        <td class="center">
                                            <label>
                                               <span class="lbl">@index</span>
                                            </label>
                                        </td>

                                        <td>
                                            @item.Operator
                                        </td>
                                        <td>@item.ChangeUserName</td>
                                        <td class="hidden-480">@item.ChangeIp</td>
                                        <td>@item.ChangeTime</td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                    
                    
                   
                </div><!-- /.table-responsive -->

            </div>
        </div>
    </div>
</div>
 