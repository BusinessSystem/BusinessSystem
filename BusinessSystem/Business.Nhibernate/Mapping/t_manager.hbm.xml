<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Business.Core.Manager, Business.Core" table="t_manager">
		<id name="Id" type="Int64" unsaved-value="0">
			<column name="ID" sql-type="bigint" not-null="true" unique="true"/>
			<generator class="native" />
		</id>
		<property name="UserName" length="200" />
		<property name="Password"/>
		<property name="ParentId"  />
		<property name="ManagerType"  />
		<property name="RealName" />
		<property name="Company" />
		<property name="IsAutoDistribute"  />
		<property name="Language"/>
		<property name="Creator"/>
		<property name="CreateTime"/>
	</class>
</hibernate-mapping>
